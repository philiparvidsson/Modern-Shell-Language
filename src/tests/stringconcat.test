name = "string concatenation"

a01 = ""
a02 = "."
a03 = "/"
a04 = "%"
a05 = "^"
a06 = "!"

bool = 1
bool *= assert(a01+a01, "")
bool *= assert(a02+a02, "..")
bool *= assert(a03+a03, "//")
bool *= assert(a04+a04, "%%")
bool *= assert(a05+a05, "^^")
bool *= assert(a06+a06, "!!")

if (bool==0) console.log(name, "failed")